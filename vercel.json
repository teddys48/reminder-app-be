{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["dist/**"] }
      },
      {
        "src": "/tmp/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      },
      {
          "src": "/(.+)",
          "dest": "/tmp/$1"
      }
    ]
  }
  